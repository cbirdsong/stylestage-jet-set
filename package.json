{
	"name": "untitled-stage",
	"version": "1.0.0",
	"description": "A style stage style.",
	"repository": {
		"type": "git",
		"url": "git+https://cbirdsong@github.com/cbirdsong/untitled-stage.git"
	},
	"author": "Cory Birdsong",
	"license": "CC0",
	"bugs": {
		"url": "https://github.com/cbirdsong/untitled-stage/issues"
	},
	"homepage": "https://github.com/cbirdsong/untitled-stage#readme",
	"scripts": {
		"build": "npm-run-all build:sass build:11ty",
		"serve": "npm-run-all --parallel watch:sass watch:11ty",
		"build:11ty": "eleventy",
		"serve:11ty": "eleventy --serve",
		"build:sass": "sass src/scss:src/css",
		"serve:sass": "sass --watch src/scss:src/css"
	},
	"devDependencies": {
		"@11ty/eleventy": "^0.11.1",
		"npm-run-all": "^4.1.5",
		"sass": "^1.29.0"
	}
}